// vim: ft=cpp

#define COMMA ,

#if defined(SHARK_MPI_COMM)

#define SYMBD(d) SYMBT(shark::ndim::part<d COMMA float>)
#include "inst_dim"
#undef SYMBD
#define SYMBD(d) SYMBT(shark::ndim::part<d COMMA double>)
#include "inst_dim"
#undef SYMBD

#elif defined(SHARK_NO_COMM)

#define SYMBD(d) SYMBT(shark::ndim::part<d COMMA float>)
#include "inst_dim"
#undef SYMBD
#define SYMBD(d) SYMBT(shark::ndim::part<d COMMA double>)
#include "inst_dim"
#undef SYMBD

#else
#error "No comm defined"
#endif

#undef COMMA
